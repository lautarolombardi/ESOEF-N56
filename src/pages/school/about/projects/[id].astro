---
import DetailPage from "@/components/common/DetailPage.astro";
import { projects } from "@/data/projects";
import Layout from "@/layouts/Layout.astro";
const { id } = Astro.params;
const project = projects.find((p) => p.id === id);
const { title, description, media, mediaType, date, link } = project;
---

<Layout title="Proyectos" description="Proyectos">
  <DetailPage
    cardType="projects"
    title={title}
    description={description}
    media={media}
    date={date}
    mediaType={mediaType}
    link={link}
  />
</Layout>
